{
  "compilerOptions": {
    "target": "esnext",
    "useDefineForClassFields": true,
    "module": "esnext",
    "moduleResolution": "node",
    "strict": true,
    "jsx": "preserve",
    "sourceMap": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "declaration": true,
    "rootDir": "./src",
    "outDir": "dist",
    "lib": ["esnext", "dom"]
  },
  // "files": ["src/core.ts", "src/element-plus.ts", "src/ant-design.ts"],
  "include": ["src/**/*.ts"],
  "exclude": ["example/**/*.vue", "example/**/*.ts"]
}
